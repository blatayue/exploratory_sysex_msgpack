    "import websocket\n",
    "def on_message(ws, message):\n",
    "    print(message)\n",
    "def on_error(wsapp, err):\n",
    "    print(\"Got an error: \", err)\n",
    "wsapp = websocket.WebSocketApp(\"ws://192.168.7.2:8080/ws\",\n",
    "on_message = on_message,\n",
    "on_error=on_error)\n",
    "wsapp.run_forever()  "
   ]
